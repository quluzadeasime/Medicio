@using Core.Models
@model Doctor

<div class="container">
    <form method="post" >
        <div class="mb-3">
            <label asp-for="FullName" class="form-label">Fullname</label>
            <input asp-for="FullName" class="form-control">
            <span asp-validation-for="FullName" style="color:red"></span>
        </div>
        <div class="mb-3">
            <label asp-for="Position" class="form-label">Position</label>
            <input asp-for="Position" class="form-control">
            <span asp-validation-for="Position" style="color:red"></span>

        </div>
        <div class="mb-3">
            <label asp-for="PhotoFile" class="form-label">ImgUrl</label>
            <input asp-for="PhotoFile" class="form-control">
            <span asp-validation-for="PhotoFile" style="color:red"></span>

        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>
